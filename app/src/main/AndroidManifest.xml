    package="com.example.gesco">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_logo_white"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_logo_white"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".User_DoctorActivity"></activity>
        <activity android:name=".Login_DoctorActivity" />
        <activity android:name=".TestAlarmActivity" />
        <activity android:name=".Login_SelectActivity" />
        <activity android:name=".PersonalData" />
        <activity android:name=".MedicationActivity" />
        <activity android:name=".ReminderActivity" />
        <activity android:name=".User_PatientActivity" />
        <activity android:name=".RegistrationActivity" />
        <activity android:name=".Login_PatientActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>